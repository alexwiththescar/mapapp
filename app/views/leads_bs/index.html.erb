<h1>Listing leads_bs</h1>
<h2>Import Leads</h2>
<%= form_tag import_leads_bs_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>
<table>
  <tr>
    <th>Address1</th>
    <th>Address2</th>
    <th>Address3</th>
    <th>Address4</th>
    <th>Firstname</th>
    <th>Homephone</th>
    <th>Lastname</th>
    <th>Postcode</th>
    <th>Price</th>
    <th>Title</th>
    <th>Address5</th>
    <th>Address6</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Gmap</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Distance</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @leads_bs.each do |leads_b| %>
  <tr>
    <td><%= leads_b.Address1 %></td>
    <td><%= leads_b.Address2 %></td>
    <td><%= leads_b.Address3 %></td>
    <td><%= leads_b.Address4 %></td>
    <td><%= leads_b.FirstName %></td>
    <td><%= leads_b.HomePhone %></td>
    <td><%= leads_b.LastName %></td>
    <td><%= leads_b.Postcode %></td>
    <td><%= leads_b.Price %></td>
    <td><%= leads_b.Title %></td>
    <td><%= leads_b.Address5 %></td>
    <td><%= leads_b.Address6 %></td>
    <td><%= leads_b.longitude %></td>
    <td><%= leads_b.latitude %></td>
    <td><%= leads_b.gmap %></td>
    <td><%= leads_b.latitude %></td>
    <td><%= leads_b.longitude %></td>
    <td><%= leads_b.Distance %></td>
    <td><%= leads_b.Price %></td>
    <td><%= link_to 'Show', leads_b %></td>
    <td><%= link_to 'Edit', edit_leads_b_path(leads_b) %></td>
    <td><%= link_to 'Destroy', leads_b, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

